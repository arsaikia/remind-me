{
  "name": "remind_me",
  "version": "1.0.0",
  "description": "This is the final project for Enterprise Web Application. The application here is a fullstact MERN with mySql and GraphQl Ecommerce Grocery Hub",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "set NODE_ENV=development&& nodemon --experimental-json-modules backend/server",
    "client": "npm start --prefix frontend",
    "start": "node  ./backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-server": "npm install",
    "start-server": "cd backend && node server.js",
    "install-client": "cd frontend && npm install",
    "build-client": "cd frontend && npm run build",
    "start-client": "cd frontend && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arsaikia/EWA_Term_Project.git"
  },
  "keywords": [
    "remind-me"
  ],
