  "bugs": {
    "url": "https://github.com/arsaikia/EWA_Term_Project/issues"
  },
  "homepage": "https://github.com/arsaikia/EWA_Term_Project#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "mongoose": "^5.10.16",
    "octokit": "^2.1.0",
    "react-cookie": "^4.1.1",
    "uuid": "^8.3.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "concurrency": "^0.1.4",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "nodemon": "^2.0.6"
  }
}
